[flake8]
ignore =
  E501,  # Line length is enforced by Black, so flake8 doesn't need to check it
  W503,  # Black disagrees with this rule, as does PEP 8; Black wins
  # TODO: Review the following rules, not in the template
  F811,
  F401,
  F405,
  E203,
exclude = 
  migrations,
  __pycache__,
  manage.py,
  settings.py,
  .venv
